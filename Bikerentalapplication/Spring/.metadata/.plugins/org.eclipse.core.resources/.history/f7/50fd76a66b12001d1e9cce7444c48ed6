import java.util.Scanner;

public class MyClass {

	public static void main(String[] args) {
		student s;
		Scanner scan=new Scanner(System.in);
		student arr[]=new student[2];
		for(int i=0;i<2;i++) {
		    String name=scan.next();
			int tam=scan.nextInt();
			int eng=scan.nextInt();
			arr[i]=new student(tam,eng,name);//arr[0]={45,55}
		}
		total t=new total();
		t.getTotal(arr);
		for(student a:arr) {
			System.out.println(a.getTotal());
		}
		ranking r=new ranking();
		arr=r.calrank(arr);
		for(student a:arr) {
			System.out.println(a.getName());
		}
	}
}
class ranking{
	public student[] calrank(student[] arr) {
		for(int i=0;i<arr.length;i++) {
			for(int j=0;j<arr.length;j++) {
			if(arr[i].getTotal()<arr[j].getTotal()) {
				student n=arr[i];
				arr[i]=arr[i+1];
				arr[i+1]=n;
			}
			}
		}		
		return arr;
	}
    	
}
class total{
		student s=new student();
	public void getTotal(student[] arr) {
		int mark=arr[0].getEng()+arr[0].getTam();
		int mark1=arr[1].getEng()+arr[1].getTam();;
		arr[0].setTotal(mark);
		arr[1].setTotal(mark1);	
	}
	
}
class student{
    private String name;
	private int tam;
	private int eng;
	private int total;
	public int getTam() {
		return tam;
	}
	public void setTam(int tam) {
		this.tam = tam;
	}
	public int getEng() {
		return eng;
	}
	public void setEng(int eng) {
		this.eng = eng;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
	public int getTotal() {
		return total;
	}
	public void setTotal(int total) {
		this.total = total;
	}
	public student(int tam, int eng,String name) {
		super();
		this.name=name;
		this.tam = tam;
		this.eng = eng;
	}
	
	public student() {
		
	}
}
